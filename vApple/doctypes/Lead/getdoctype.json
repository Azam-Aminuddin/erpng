on, __(\"Make\"));\n\t\t\tcur_frm.page.set_inner_btn_group_as_primary(__(\"Make\"));\n\t\t}\n\n\t\tif(!this.frm.doc.__islocal) {\n\t\t\tfrappe.contacts.render_address_and_contact(cur_frm);\n\t\t} else {\n\t\t\tfrappe.contacts.clear_address_and_contact(cur_frm);\n\t\t}\n\t},\n\n\tcreate_customer: function() {\n\t\tfrappe.model.open_mapped_doc({\n\t\t\tmethod: \"erpnext.crm.doctype.lead.lead.make_customer\",\n\t\t\tfrm: cur_frm\n\t\t})\n\t},\n\n\tcreate_opportunity: function() {\n\t\tfrappe.model.open_mapped_doc({\n\t\t\tmethod: \"erpnext.crm.doctype.lead.lead.make_opportunity\",\n\t\t\tfrm: cur_frm\n\t\t})\n\t},\n\n\tmake_quotation: function() {\n\t\tfrappe.model.open_mapped_doc({\n\t\t\tmethod: \"erpnext.crm.doctype.lead.lead.make_quotation\",\n\t\t\tfrm: cur_frm\n\t\t})\n\t}\n});\n\n$.extend(cur_frm.cscript, new erpnext.LeadController({frm: cur_frm}));\n\n\n\n","__tree_js":null,"docstatus":0,"sort_field":"modified","__messages":null,"image_field":"image","allow_copy":0,"allow_guest_to_view":0,"istable":0,"has_web_view":0,"__map_js":null,"__css":null,"show_name_in_global_search":1,"beta":0,"read_only_onload":0,"module":"CRM","doctype":"DocType","autoname":"naming_series:","__kanban_boards":[],"issingle":0,"icon":"fa fa-user","__listview_template":null,"name":"Lead","idx":5,"hide_toolbar":0,"__workflow_docs":[],"title_field":"lead_name","modified":"2017-08-21 02:28:21.581948","is_submittable":0,"name_case":"Title Case","__calendar_js":null,"__list_js":null,"permissions":[{"creation":"2013-04-10 11:45:37","share":0,"doctype":"DocPerm","owner":"Administrator","export":0,"cancel":0,"set_user_permissions":0,"modified_by":"Administrator","create":0,"submit":0,"write":0,"role":"All","print":0,"docstatus":0,"permlevel":1,"parent":"Lead","read":1,"import":0,"report":1,"apply_user_permissions":0,"name":"2d8e19f7ef","idx":1,"amend":0,"modified":"2017-09-29 01:32:34.022893","email":0,"parenttype":"DocType","parentfield":"permissions","if_owner":0,"delete":0},{"creation":"2013-0