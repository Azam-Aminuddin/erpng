.on(\"System Settings\", \"enable_password_policy\", function(frm) {\n\tif(frm.doc.enable_password_policy == 0){\n\t\tfrm.set_value(\"minimum_password_score\", \"\");\n\t} else {\n\t\tfrm.set_value(\"minimum_password_score\", \"2\");\n\t}\n});\n\nfrappe.ui.form.on(\"System Settings\", \"enable_two_factor_auth\", function(frm) {\n\tif(frm.doc.enable_two_factor_auth == 0){\n\t\tfrm.set_value(\"bypass_2fa_for_retricted_ip_users\", 0);\n\t}\n});","__tree_js":null,"docstatus":0,"__messages":null,"allow_copy":0,"allow_guest_to_view":0,"istable":0,"has_web_view":0,"__map_js":null,"__css":null,"show_name_in_global_search":0,"beta":0,"read_only_onload":0,"module":"Core","doctype":"DocType","__dashboard":{},"__kanban_boards":[],"issingle":1,"icon":"fa fa-cog","__listview_template":null,"name":"System Settings","idx":0,"hide_toolbar":0,"__workflow_docs":[],"modified":"2017-09-13 13:26:11.045262","is_submittable":0,"name_case":"","__calendar_js":null,"permissions":[{"creation":"2014-04-17 16:53:52.640856","share":1,"doctype":"DocPerm","owner":"Administrator","export":0,"cancel":0,"set_user_permissions":0,"modified_by":"Administrator","create":1,"submit":0,"write":1,"role":"System Manager","print":0,"docstatus":0,"permlevel":0,"parent":"System Settings","read":1,"import":0,"report":0,"apply_user_permissions":0,"name":"deaa893f48","idx":1,"amend":0,"modified":"2017-09-29 01:32:06.365914","email":0,"parenttype":"DocType","parentfield":"permissions","if_owner":0,"delete":0}],"quick_entry":1,"__custom_js":"","__print_formats":[]}],"user_settings":"{}"}