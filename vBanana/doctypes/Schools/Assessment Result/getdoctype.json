unction(r) {\r\n\t\t\t\tif (r.message) {\r\n\t\t\t\t\tfrm.doc.details \u003d [];\r\n\t\t\t\t\t$.each(r.message, function(i, d) {\r\n\t\t\t\t\t\tvar row \u003d frappe.model.add_child(frm.doc, \"Assessment Result Detail\", \"details\");\r\n\t\t\t\t\t\trow.assessment_criteria \u003d d.assessment_criteria;\r\n\t\t\t\t\t\trow.maximum_score \u003d d.maximum_score;\r\n\t\t\t\t\t});\r\n\t\t\t\t}\r\n\t\t\t\trefresh_field(\"details\");\r\n\t\t\t}\r\n\t\t});\r\n\t}\r\n});\r\n\r\nfrappe.ui.form.on(\"Assessment Result Detail\", {\r\n\tscore: function(frm, cdt, cdn) {\r\n\t\tvar d  \u003d locals[cdt][cdn];\r\n\t\tif (d.score \u003e\u003d d.maximum_score) {\r\n\t\t\tfrappe.throw(__(\"Score cannot be greater than Maximum Score\"));\r\n\t\t}\r\n\t\telse {\r\n\t\t\tfrappe.call({\r\n\t\t\t\tmethod: \"erpnext.schools.api.get_grade\",\r\n\t\t\t\targs: {\r\n\t\t\t\t\tgrading_scale: frm.doc.grading_scale,\r\n\t\t\t\t\tpercentage: ((d.score/d.maximum_score) * 100)\r\n\t\t\t\t},\r\n\t\t\t\tcallback: function(r) {\r\n\t\t\t\t\tif (r.message) {\r\n\t\t\t\t\t\tfrappe.model.set_value(cdt, cdn, \"grade\", r.message);\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t});\r\n\t\t}\r\n\t}\r\n});"},{"allow_copy":0.0,"allow_guest_to_view":0.0,"allow_import":0.0,"allow_rename":0.0,"autoname":"","beta":0.0,"creation":"2016-12-14 17:44:35.583123","custom":0.0,"docstatus":0.0,"doctype":"DocType","document_type":"","editable_grid":1.0,"engine":"InnoDB","fields":[{"allow_bulk_edit":0.0,"allow_on_submit":0.0,"bold":0.0,"collapsible":0.0,"columns":4.0,"fieldname":"assessment_criteria","fieldtype":"Link","hidden":0.0,"ignore_user_permissions":0.0,"ignore_xss_filter":0.0,"in_filter":0.0,"in_global_search":0.0,"in_list_view":1.0,"in_standard_filter":0.0,"label":"Assessment Criteria","length":0.0,"no_copy":0.0,"options":"Assessment Criteria","permlevel":0.0,"precision":"","print_hide":0.0,"print_hide_if_no_value":0.0,"read_only":1.0,"remember_last_selected_value":0.0,"report_hide":0.0,"reqd":1.0,"search_index":0.0,"set_only_